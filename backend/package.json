{
  "name": "ledy-management-backend",
  "version": "1.0.0",
  "description": "Ledy management backend",
  "main": "./build/server",
  "scripts": {
    "build": "npx -y tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:reset": "npm run build && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prestart": "npm run build",
    "start": "node .",
    "predev": "npm run db:reset",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts"
  },
  "author": "Victor Hugo",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "@types/sequelize": "^4.28.14",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "sequelize-cli": "^6.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "sequelize": "^6.28.0"
  }
}
