{
  "name": "ledy-management-backend",
  "version": "1.0.0",
  "description": "Ledy management backend",
  "main": "./build/server",
  "scripts": {
    "build": "npx -y tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:reset": "npm run build && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prestart": "npm run build",
    "start": "node .",
    "predev": "npm run db:reset",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts"
  },
  "author": "Victor Hugo",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "@types/sequelize": "^4.28.14",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "sequelize": "^6.28.0"
  }
}
